{
  "name": "@voiceflow/common",
  "version": "0.0.1",
  "description": "Junk drawer of utility functions",
  "main": "index.js",
  "scripts": {
    "test-all": "nyc mocha --use_strict \"tests/**/*.it.js\" \"tests/**/*.unit.js\"",
    "lint": "eslint \"**/*.js\" --fix",
    "test": "npm run unit && npm run lint",
    "unit": "nyc mocha --use_strict --exit \"tests/**/*.unit.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storyflow/common.git"
  },
  "keywords": [
    "utilities"
  ],
  "author": "Voiceflow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/storyflow/common/issues"
  },
  "files": [
    "lib/**",
    "config/**",
    "app/**",
    "*.js",
    "!scratch.*"
  ],
  "nyc": {
    "exclude": [
      "coverage/**",
      "tests/**",
      "scratch.js",
      "index.js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ],
    "all": true
  },
  "homepage": "https://github.com/storyflow/common#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^7.5.0",
    "@voiceflow/eslint-config": "0.0.1",
    "chai": "^4.2.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "fixpack": "^2.3.1",
    "husky": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "semantic-release": "^15.13.3"
  }
}
